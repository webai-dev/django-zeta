          <template>default-template</template>
          <theme>default-theme</theme>
          <blocks>
            <item>
              <name>Content</name>
              <translations>
                <item>
                  <language>en</language>
                  <frontend>Web</frontend>
                  <content>you search for the money...&lt;br/&gt;you find it!&lt;br/&gt;but someone else found it first.&lt;br/&gt;you ask them for it back.&lt;br/&gt;they give you half.&lt;br/&gt;&lt;br/&gt;<Input.Search/></content>
                </item>
              </translations>
            </item>
            <item>
              <name>Picture</name>
              <translations>
                <item>
                  <language>en</language>
